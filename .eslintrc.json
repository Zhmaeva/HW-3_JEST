{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "jest/globals": true
    },
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "overrides": [
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "semi": "error",
      "no-dupe-else-if": "error",
      "no-duplicate-case": "error",
      "prefer-const": "error",
      "no-duplicate-imports": "error",
      "no-unreachable": "error",
      "no-unused-vars": "warn"
    }
  }